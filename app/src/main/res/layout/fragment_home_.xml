<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home_Fragment">

    <ImageView
        android:id="@+id/home_main_image1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="90.4dp"
        android:src="@drawable/ic_example"
        app:layout_constraintHeight_percent="0.241"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.884" />

    <TextView
        android:id="@+id/home_today_todo_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="36dp"
        android:lineSpacingExtra="5sp"
        android:text="오늘 설정한 To-do List"
        android:textColor="#2a2a2a"
        android:textSize="24sp"
        android:textStyle="normal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/home_main_image1" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/home_today_todo_layout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="12dp"
        android:background="#ffffff"
        app:layout_constraintHeight_percent="0.205"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/home_today_todo_title"
        app:layout_constraintWidth_percent="0.884"></androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/home_yesterday_todo_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="20dp"
        android:lineSpacingExtra="5sp"
        android:text="어제 달성못한 To-do list"
        android:textColor="#bababa"
        android:textSize="24sp"
        android:textStyle="normal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/home_today_todo_layout" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="12dp"
        android:background="#ffffff"
        app:layout_constraintHeight_percent="0.161"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/home_yesterday_todo_title"
        app:layout_constraintWidth_percent="0.884"></androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>